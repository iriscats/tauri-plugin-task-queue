{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "demo",
    "version": "1.0.0",
    "identifier": "com.tauri.task.queue.demo",
    "build": {
        "beforeDevCommand": "pnpm dev",
        "devUrl": "http://localhost:3000",
        "beforeBuildCommand": "pnpm build",
        "frontendDist": "../"
    },
    "app": {
        "withGlobalTauri": true,
        "windows": [
            {
                "url": "index.html",
                "title": "main",
                "width": 1200,
                "height": 1200
            }
        ],
        "security": {
            "csp": null,
            "assetProtocol": {
                "scope": [
                    "**/*"
                ],
                "enable": true
            }
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/icon.png"
        ],
        "createUpdaterArtifacts": true
    }
}