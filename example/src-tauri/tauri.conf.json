{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "demo",
    "version": "1.0.0",
    "identifier": "com.tauri.task.queue.demo",
    "build": {
        "beforeDevCommand": "pnpm dev",
        "devUrl": "http://localhost:3000",
        "beforeBuildCommand": "pnpm build",
        "frontendDist": "../"
    },
    "app": {
        "windows": [
            {
                "url": "index.html",
                "title": "demo",
                "width": 1200,
                "height": 628
            }
        ]
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/icon.png"
        ],
        "createUpdaterArtifacts": true
    }
}